{
  "pageUrl": "/sharedregistration",
  "elements": {
    "registrationTitle": "css=.icon.account",
    "textFields": "css=input[type='text']",
    "passwordFields": "css=input[type='password']",
    "firstNameField": "id=FirstName",
    "lastNameField": "id=LastName",
    "emailAddressField": "id=EmailAddress",
    "passwordField": "id=Password",
    "confirmPasswordField": "id=ConfirmPassword",
    "cardTypeDropdown": "id=CardType",
    "cardNumberField": "id=CardNumber",
    "securityCodeField": "id=CardIdNumber",
    "expirationMonthDropdown": "id=ExpirationDateMonth",
    "expirationYearDropdown": "id=ExpirationDateYear",
    "countryDropdown": "id=BillingCountry",
    "zipCodeField": "id=BillingZip",
    "termsOfUseCheckbox": "css=.row.termsOfService>input",
    "signUpNowButton": "css=.submit_button_image",
    "cardTypeDefaultValue": "css=#CardType>option:nth-of-type(1)",
    "monthDefaultValue": "css=#ExpirationDateMonth>option:nth-of-type(1)",
    "yearDefaultValue": "css=#ExpirationDateYear>option:nth-of-type(1)",
    "countryDefaultValue": "css=#BillingCountry>option:nth-of-type(1)",
    "errorMessage": "css=.field-validation-error>span",
    "enterFirstNameMessage": "css=.field-validation-error>span[for='FirstName']",
    "enterLastNameMessage": "css=.field-validation-error>span[for='LastName']",
    "enterEmailMessage": "css=.field-validation-error>span[for='EmailAddress']",
    "enterPasswordMessage": "css=.field-validation-error>span[for='Password']",
    "confirmPasswordMessage": "css=.field-validation-error>span[for='ConfirmPassword']",
    "enterCardNumberMessage": "css=.field-validation-error>span[for='CardNumber']",
    "enterCodeMessage": "css=.field-validation-error>span[for='CardIdNumber']",
    "selectMonthMessage": "css=.field-validation-error>span[for='ExpirationDateMonth']",
    "selectYearMessage": "css=.field-validation-error>span[for='ExpirationDateYear']",
    "enterBillingZipCodeMessage": "css=.field-validation-error>span[for='BillingZip']",
    "errorMessageForTermsOfUse": "id=toserror",
    "expirationDateMessage": "css=.field-validation-error",
    "billingPlan": "id=fineprint",
    "confirmEmailField": "id=ConfirmEmailAddress"
  }
}