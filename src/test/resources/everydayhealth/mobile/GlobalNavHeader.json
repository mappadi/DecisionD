    "searchIcon": "css=.eh-header__button--search-toggle i",
    "profileMenu": "css=div.eh-profile",
    "hamburgerMenuCloseIcon": "css=.eh-header__button--menu-toggle i span.eh-header__icon",
    "myProfileCloseIcon": "css=.dropdown-toggle__section--logged-in i span.eh-header__icon",
    "searchCloseIcon": "css=.eh-header__button--search-toggle i span.eh-header__icon",
    "searchOverlay": "css=div.search"
